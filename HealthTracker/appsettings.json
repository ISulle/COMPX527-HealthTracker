{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AWS": {
    "Region": "ap-southeast-2",
    "Profile": "default",
    "Cognito": {
      "ClientId": "<ClientId>",
      "ClientSecret": "<ClientSecret>",
      "MetadataAddress": "https://cognito-idp.ap-southeast-2.amazonaws.com/<MetadataAddress>/.well-known/openid-configuration",
      "ResponseType": "code",
      "SaveToken": true,
      "AppSignOutUrl": "/Account/SignOutSuccessful",
      "CognitoDomain": "https://<CognitoDomain>.auth.ap-southeast-2.amazoncognito.com"
    }
  }
}
